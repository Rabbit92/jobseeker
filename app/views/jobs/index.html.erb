<h1>Listing jobs</h1>

<table>
  <thead>
    <tr>
      <th>Nivel cariera</th>
      <th>Descriere</th>
      <th>Competente obligatorii</th>
      <th>Competente optionale</th>
      <th>Beneficii</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobs.each do |job| %>
      <tr>
        <td><%= job.nivel_cariera %></td>
        <td><%= job.descriere %></td>
        <td><%= job.competente_obligatorii %></td>
        <td><%= job.competente_optionale %></td>
        <td><%= job.beneficii %></td>
        <td><%= link_to 'Show', job %></td>
        <td><%= link_to 'Edit', edit_job_path(job) %></td>
        <td><%= link_to 'Destroy', job, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job', new_job_path %>
