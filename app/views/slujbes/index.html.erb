<h1>Listing slujbes</h1>

<table>
  <thead>
    <tr>
      <th>Nivel cariera</th>
      <th>Descriere</th>
      <th>Competente obligatorii</th>
      <th>Competente optionale</th>
      <th>Beneficii</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @slujbes.each do |slujbe| %>
      <tr>
        <td><%= slujbe.nivel_cariera %></td>
        <td><%= slujbe.descriere %></td>
        <td><%= slujbe.competente_obligatorii %></td>
        <td><%= slujbe.competente_optionale %></td>
        <td><%= slujbe.beneficii %></td>
        <td><%= link_to 'Show', slujbe %></td>
        <td><%= link_to 'Edit', edit_slujbe_path(slujbe) %></td>
        <td><%= link_to 'Destroy', slujbe, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Slujbe', new_slujbe_path %>
